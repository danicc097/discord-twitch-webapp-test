{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "pnpm run prisma:migrate && next build",
    "prisma-dev": "dotenv -e .env.development -- prisma",
    "prisma-dev:reset": "dotenv -e .env.development -- prisma migrate reset",
    "prisma-dev:deploy": "dotenv -e .env.development -- prisma migrate deploy",
    "prisma-dev:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "prisma-dev:generate": "dotenv -e .env.development -- prisma generate",
    "prisma-dev:create-migration": "dotenv -e .env.development -- prisma migrate dev --create-only",
    "prisma:migrate": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --config ./.eslintrc.json --ext .js,.jsx,.ts,.tsx --fix ."
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.cjs"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "next": "13.1.1",
    "shared-types": "file:../types",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "prisma": "^4.8.0",
    "shared-types": "file:../types"
  }
}
