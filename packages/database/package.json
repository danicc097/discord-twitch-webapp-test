{
  "name": "database",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "private": true,
  "scripts": {
    "db:generate": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma generate --data-proxy",
    "db:push": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma db push --skip-generate --accept-data-loss --force-reset",
    "db:migrate-dev": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate dev",
    "db:seed": "prisma db seed",
    "prod-rollback": ". ./.env && DATABASE_URL=\"$MIGRATE_PROD_DATABASE_URL\" prisma migrate resolve --rolled-back ",
    "prisma-dev": "prisma",
    "prisma-dev:reset": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate reset",
    "prisma-dev:create-migration": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate dev --create-only",
    "prisma:migrate": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma generate --data-proxy && DATABASE_URL=\"$MIGRATE_DATABASE_URL\" npx prisma migrate deploy",
    "lint": "eslint --config ./.eslintrc.js --ext .js,.jsx,.ts,.tsx --fix ."
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "prisma": "^4.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "eslint-config-custom": "workspace:*",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.1"
  }
}
