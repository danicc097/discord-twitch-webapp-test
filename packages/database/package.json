{
  "name": "database",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "private": true,
  "scripts": {
    "db:generate": "dotenv -e .env.development -- prisma generate",
    "db:push": "dotenv -e .env.development -- prisma db push --skip-generate --accept-data-loss --force-reset",
    "db:migrate-dev": "dotenv -e .env.development -- prisma migrate dev",
    "db:seed": "dotenv -e .env.development -- prisma db seed",
    "prisma-dev": "dotenv -e .env.development -- prisma",
    "prisma-dev:reset": "dotenv -e .env.development -- prisma migrate reset",
    "prisma-dev:deploy": "dotenv -e .env.development -- prisma migrate deploy",
    "prisma-dev:generate": "dotenv -e .env.development -- prisma generate",
    "prisma-dev:create-migration": "dotenv -e .env.development -- prisma migrate dev --create-only",
    "prisma:migrate": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy",
    "lint": "eslint --config ./.eslintrc.js --ext .js,.jsx,.ts,.tsx --fix ."
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "@faker-js/faker": "^7.6.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "lodash": "^4.17.21",
    "prisma": "^4.8.0",
    "ts-node": "^10.9.1"
  }
}
